<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Express Dev-skills</title>
</head>
<body>
    <h1>Dev-skills</h1>
    <a href="/devSkills/new">Add a skill</a>
    <ul>
        <% devSkills.forEach(function(s) { %>
            <a href="/devSkills/<%=s.id%>"><li><%= s.devSkill %></li></a>
        <% }); %>
    </ul>
</body>
</html>